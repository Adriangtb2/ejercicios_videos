Algoritmo sin_titulo
	definir a,b,c,resultado como reales;
	
	escribir "diguite el valor de A: ";
	leer a;
	escribir "diguite el valor de B: ";
	leer b;
	escribir "diguite el valor de c: ";
	leer c;
	
	resultado <- (-b + rc(b*b - 4*a*c))/(2*a);
	escribir "el resultado es: ",resultado;
FinAlgoritmo 
